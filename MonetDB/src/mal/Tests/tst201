## Lets cut out a few sections
	_v38:= select(b,2,4);
	_v39:= print(_v38);
#b.select(2,4).print;
	_v40:= int(nil);
	_v41:= select(b,7,_v40);
	_v42:= print(_v41);
#b.select(7,int(nil)).print;
	_v43:= int(nil);
	_v44:= int(nil);
	_v45:= select(b,_v43,_v44);
	_v46:= print(_v45);
#b.select(int(nil),int(nil)).print;
