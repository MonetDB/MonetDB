bint:= new(void,int);
setSequenceBase(bint,0@0);
i:= -1;
barrier m:=nextElement(i,1,10);
	insert(bint,nil,i);
	redo m;
end	m;
print(bint);
dint := multiplex.script("decimal",bint);
print(dint);
print("start division");
d2:= decimal(2);
print(d2);
#zint:= multiplex.script("/",dint,d2);
zint:= new(void,decimal);
setSequenceBase(zint,0@0);
barrier mloop := mal.bunStream(dint,h:void,B2:decimal);
	print(B2);
	cr := decimal./(B2,d2);
	print(cr);
	insert(zint,nil,cr);
	redo mloop;
end	mloop;
	
