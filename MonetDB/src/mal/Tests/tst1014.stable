	bat.insert(b,1,2);	#7:($9:bat[int,int])<-(b:bat[int,int])($10:int)($11:int)
	bbp.deposit("myfirstbat",b);	#8:($12:nil)<-($13:str)(b:bat[int,int])
	c := bbp.take("myfirstbat");	#9:(c:bat[any,any])<-($13:str)
	io.print(c);	#10:($15:int)<-(c:bat[any,any])
	bbp.close();	#11:($16:nil)
