# what is the overhead of MAL in multiplex situations
# using a BATloop !! This is 40x slower as the V4.3 multiplex implementation
b:= new(lng,lng);
i:= -1:lng;
barrier v:= nextElement(i,0:lng,10:lng);
	insert(b,i,i);
	redo v;
end v;
c:= copy(b);

d:= new(lng,lng);
barrier w:= bunStream(b,h:lng,t:lng);
	B1:= find(b,h);
	B2:= find(c,h);
	t:= B1+B2;
	insert(d,h,t);
	redo w;
catch 	GDKerror;
end	GDKerror;
end w;
print(d);
