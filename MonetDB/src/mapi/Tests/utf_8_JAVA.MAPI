#!/bin/sh

cp=""
if [ -f "$MONETDB_BUILD/src/mapi/clients/java/mapi.jar" ]; then
	cp="`$CYGPATH_WP "$MONETDB_BUILD/src/mapi/clients/java/mapi.jar"`"
elif [ -f "$MONETDB_PREFIX/lib/MonetDB/java/mapi.jar" ]; then
	cp="`$CYGPATH_WP "$MONETDB_PREFIX/lib/MonetDB/java/mapi.jar"`"
else
	echo "Neither $MONETDB_BUILD/src/mapi/clients/java/mapi.jar nor $MONETDB_PREFIX/lib/MonetDB/java/mapi.jar found"
	exit 1
fi

LANG="en_US.UTF-8" 
export LANG

java -cp "$cp" MapiClient localhost $MAPIPORT << EOMIL
	print("\303\251\303\251n");
EOMIL
