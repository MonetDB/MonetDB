# create large bats bc (chr) and bl (lng)
var bc, bl; 
if (isnil(CATCH(count(bat("bc"))))) {
  bc := bat("bc");
  bl := bat("bl");
} else { 
  var bak := debugmask();
  debugmask(0);
  var j := 0;
  var ac := bat(void,chr,128).seqbase(0@0);
  var al := bat(void,lng,128).seqbase(0@0);
  while(j < 128) {
    ac.append(chr(j));
    al.append(lng(j));
    j :+= 1;
  }
  var i := 0LL;
  var n := lng(100*REMAP_PAGE_SIZE);
  bc := bat(void,chr,n - 1LL).mmap(1).seqbase(0@0).rename("bc");
  bl := bat(void,lng,n - 1LL).mmap(1).seqbase(0@0).rename("bl");
  while((i :+= 128LL) <= n) {
     bc.append(ac);
     bl.append(al);
  }
  bc.persists(true).access(BAT_READ);
  bl.persists(true).access(BAT_READ);
  debugmask(bak);
}

