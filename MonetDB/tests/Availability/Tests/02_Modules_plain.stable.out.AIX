Plain_mods:=MODULES.select("plain").copy.access(BAT_WRITE).delete("mprof","plain").sort.copy.access(BAT_WRITE).insert("mprof","plain");
table(Plain_mods.reverse.col_name(sprintf("%i modules",Plain_mods.count)));
