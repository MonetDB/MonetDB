# all
# >Description:
# Try this out:
# > a := new(int,str);
# > a.insert(1,"hello world");
# > a.seqbase(100@0);
# > [oid]([a~]).reverse.print;
# #-----------------#
# # oid   | tmp_92  #
# #-----------------#
# [ 100@0,  1       ]
# > [oid]([a~]).reverse.copy.print;
# #-----------------#
# # oid   | tmp_93  #
# #-----------------#
# [ 1@0,    100     ]
# 

setoid(oid(20000000));
a := new(int,str);
a.insert(1,"hello world");
a.print;
[a~].print;
[a~].reverse.print;
[oid]([a~]).reverse.print;
a.seqbase(100@0);
[oid]([a~]).reverse.print;
[oid]([a~]).reverse.copy.print;
quit;
