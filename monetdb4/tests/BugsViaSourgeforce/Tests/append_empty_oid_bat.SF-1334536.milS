var bar := bat(void,void).insert(nil,nil).mark(0@0).reverse().mark(0@0);
bar := bar.reverse().mark(0@0).reverse();
var foo := bat(oid,oid);
foo.append(bar);
foo.print();
var foo := bat(oid,oid);
foo.insert(bar);
foo.print();

var a := bat(void,oid).insert(nil,1@0).insert(nil,2@0).reverse();
