setoid(oid(20000000));

var MODULES:=bat("MODULES").copy();

PROC mmn() : BAT[oid,str] {
	 return(monet_mod_nme.kdiff(monet_mod_nme.[startsWith]("_").uselect(true)));
}

#All_mods:=MODULES.sort().copy();
# # module(Meta);
# skip pftijah & xrpc_server as they require pathfinder to be loaded first
var All_mods:=MODULES.access(BAT_WRITE).delete("pftijah").delete("xrpc_server").sort().access(BAT_WRITE).revert();
All_mods@batloop() {
	printf("%9s: ",$h);
	if (not(monet_mod_nme.reverse().exist($h))) {
		printf("loading... ");
		module($h);
		printf("done.\n");
	} else {
		printf("loaded.\n");
	}
}

table(mmn().reverse().sort().reverse().col_name(sprintf("%i modules",mmn().count())));

