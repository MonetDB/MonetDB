module(radix);

mem_bigsize(1024LL);
vm_minsize(1024LL);

PROC killer_init(int count) : void
{
        destroy("killer_bat");
        commit();

        var b := uniform(count);
        b.persists(true).rename("killer_bat");
        commit();
}

PROC killer_init() : void
{
        killer_init(1024*1024);
}

PROC killer_test() : void
{
        bat("killer_bat").info().find("batBuns.storage").print();
}
