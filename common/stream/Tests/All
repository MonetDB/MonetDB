read_uncompressed
HAVE_LIBBZ2?read_bz2
HAVE_LIBZ?read_gz
HAVE_LIBLZ4&HAVE_PYTHON_LZ4?read_lz4
HAVE_LIBLZMA?read_xz

write_uncompressed
HAVE_LIBBZ2?write_bz2
HAVE_LIBZ?write_gz
HAVE_LIBLZ4&HAVE_PYTHON_LZ4?write_lz4
HAVE_LIBLZMA?write_xz

HAVE_CURL?urlstream
read_iconv
write_iconv

HAVE_LIBBZ2?read_concatenated_bz2
HAVE_LIBZ?read_concatenated_gz
HAVE_LIBLZ4&HAVE_PYTHON_LZ4?read_concatenated_lz4
HAVE_LIBLZMA?read_concatenated_xz
