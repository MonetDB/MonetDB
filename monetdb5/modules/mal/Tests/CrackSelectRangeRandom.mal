# The first reference case for fast cracking

include microbenchmark;
include crackers;

tuples:= 10000000;
values:= tuples;
b:= microbenchmark.uniform(0@0,tuples,values);
tf:=0:lng;
n:=50;
barrier (go,i):= (true,0);
	low:int:= mmath.rand();
	low := low % values;
	hgh:int:= mmath.rand();
	hgh := hgh % values;
	redo go := low>hgh;
	#io.printf("Low: %d",low);
	#io.printf("  High: %d  ",hgh);
t := alarm.usec();
	c:=crackers.select(b,low,hgh,true,false);
t1:= alarm.usec();
m:=t1-t;
tf:=tf+m;
io.print(m);
	redo (go,i):= language.nextElement(1,n);
exit (go,i);
io.print("Total time with cracking:",tf);
