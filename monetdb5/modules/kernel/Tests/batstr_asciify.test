statement ok
create table bar(y string);

statement ok
insert into bar values ('Olá Mundo!'),('João'),('Parabéns Ri!'),('Müller')

onlyif system=Linux
query T
select asciify(y) from bar;
----
Ola Mundo!
Joao
Parabens Ri!
Muller

skipif system=Linux
query T
select asciify(y) from bar;
----
Ol'a Mundo!
Jo~ao
Parab'ens Ri!
M"uller

statement ok
drop table bar;

