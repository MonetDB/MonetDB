calc_export str CALCbat2batid(int *ret, int *bid);
str
CALCbat2batid(bat *ret, int *bid)
{
	BAT *b;

	if( *bid == bat_nil){
		*ret= bat_nil;
		return MAL_SUCCEED;
	}
	b = BATdescriptor(*bid);
	if (b == 0)
		throw(MAL, "calc.getBAT", RUNTIME_OBJECT_MISSING);
	*ret = b->batCacheid;
	BBPkeepref(b->batCacheid);
	return MAL_SUCCEED;
}

