		address CALCbinaryXOR@1@1;
		command not(left:@1) :@1 
		address CALCunary@1NOT;
		command <<(left:@1,right:int) :@1 
		address CALCbinaryLSH@1int;
		command >>(left:@1, right:int) :@1 
		address CALCbinaryRSH@1int;
