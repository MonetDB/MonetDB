 * Coercions generally do not check on information loss
 * @-
@= mal_coercion_impl
calc_export str CALC@22@1(@1 *res, @2 *a);
str CALC@22@1(@1 *res, @2 *a)
{
	if (*a == @2_nil @3) {
		*res= @1_nil;
/*	obey semantics above, don't check for information loss
		if (*a != @2_nil)
			throw(MAL, "calc.@1", ARGUMENT_TOO_LARGE);
*/
	} else {
		*res= @4;
	}
	return MAL_SUCCEED;
}
