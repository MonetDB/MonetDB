# Yield commands may only appear in factories

factory random(seed:int):int;
	i:= seed;
barrier always:= true;
	i:= 125 *i;
	i:= i % 32767;
	yield i;
	redo always;
exit always;
	return random:= 0;
end random;

r1:= user.random(0);
io.print(r1);
r2:= user.random(0);
io.print(r2);
factories.shutdown("user","random");
