# dealing with BAT references in functions

function dir():void;
	d:= bbp.getRefCount();
	dl:=bbp.getLRefCount();
	dn:= bbp.getNames();
	ds:= algebra.sort(dn,false,false,false);
	io.print(ds,d,dl);
end dir;


function tmp1(b:bat[:any_2]):bat[:any_2];
	tmp1:= algebra.copy(b);
	io.print("inside tmp1");
	i:= bbp.getLRefCount(b);
	io.print(i);
	return tmp1;
end tmp1;

