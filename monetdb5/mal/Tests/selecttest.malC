function user.doall ( b:bat[:int] , n:int , m1:int ) :void;

	io.printf("\nm = %d\n", m1);

	m2 := m1 / 2;

	x1 := 0 - m1;
	x2 := 0 - 1;
	user.doit ( b , n , x1 , x2 );

	x1 := nil:int;
	x2 := 0 + m1;
	user.doit ( b , n , x1 , x2 );

	x1 := 0 + 1;
	x2 := 0 + m1;
	user.doit ( b , n , x1 , x2 );

	x := n / 4;
	x1 := x - m2;
	x2 := x + m2;
	user.doit ( b , n , x1 , x2 );

	x := n / 2;
	x1 := x - m2;
	x2 := x + m2;
	user.doit ( b , n , x1 , x2 );

	x := n / 4;
	x := x * 3;
	x1 := x - m2;
	x2 := x + m2;
	user.doit ( b , n , x1 , x2 );

	x1 := n - m1;
	x2 := n - 1;
	user.doit ( b , n , x1 , x2 );

	x1 := n - m1;
	x2 := nil:int;
	user.doit ( b , n , x1 , x2 );

	x1 := n + 1;
	x2 := n + m1;
	user.doit ( b , n , x1 , x2 );

end doall;


