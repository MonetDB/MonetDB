b := bat.new(:oid,:oid);
bat.insert(b,0@0, 0@0);
bat.insert(b,1@0, 1@0);
bat.insert(b,1@0, 2@0);
bat.insert(b,1@0, 3@0);
bat.insert(b,2@0, 2@0);
bat.insert(b,2@0, 0@0);
bat.insert(b,2@0, 3@0);
