bt:=algebra.markH(b);
hb:=algebra.markT(b);
bh:=bat.reverse(hb);
(e,m):=group.new(bh);

i:=aggr.min(bt,m,e);
g:=algebra.join(hb,i);
io.print(g);

a:=aggr.max(bt,m,e);
