	b:= bat.new(:oid);
	c:= bat.new(:oid);
	j1:= algebra.projection(a,b);
	j2:= algebra.projection(c,j1);
	io.print(j2);
end tst0;

function tst1();
	a:= bat.new(:oid);
	b:= bat.new(:oid);
	c:= bat.new(:oid);
	j1:= algebra.leftjoin(a,b);
	j2:= algebra.leftjoin(c,j1);
	io.print(j2);
end tst1;

