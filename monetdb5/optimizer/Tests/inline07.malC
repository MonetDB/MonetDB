# remapping inline functions
inline function foo(a:int,b:bat[:int]):dbl;
	s:= aggr.sum(b);
	c:= a+s;
	return foo:=c;
end foo;

function qry();
	b:= bat.new(:int);
	bat.append(b,1);
	z:bat[:dbl]:= mal.multiplex("user","foo",b,b);
end qry;

	optimizer.inline("user","qry");
