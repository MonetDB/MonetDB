
# remapping inline functions
inline function foo(a:str):str;
	a:= str.replace(a,"A","");
	a:= str.replace(a,"E","");
	a:= str.replace(a,"I","");
	a:= str.replace(a,"O","");
	a:= str.replace(a,"U","");
	a:= str.replace(a," ","");
	return foo:=a;
end foo;

inline function bar(a:str,b:str):str;
	sa:= user.foo(a);
	sb:= user.foo(b);
	c:= sa+sb;
	return bar:=c;
end bar;

function qry();
