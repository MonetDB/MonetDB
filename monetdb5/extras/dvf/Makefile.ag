	../../optimizer \
	../../mal \
	../../../gdk \
	../../../common/stream \
	../../../common/options \

MTSAFE

lib_opt_dvf = {
	MODULE
	DIR = libdir/monetdb5
	SEP = _
	SOURCES = opt_dvf.mx
	LIBS = ../../tools/libmonetdb5 \
		../../../gdk/libbat \
		../../../common/stream/libstream \
		$(MALLOC_LIBS)
}

lib__dvf = {
	MODULE
	DIR = libdir/monetdb5
