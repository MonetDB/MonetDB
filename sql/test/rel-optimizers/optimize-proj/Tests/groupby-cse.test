query T nosort
PLAN
SELECT
    foo.c2 AS col, 
    count(*) 
FROM 
    foo JOIN bar 
ON 
    foo.c2 = bar.c2 
GROUP BY
    foo.c2, 
    col
----
project (
| group by (
| | join (
| | | table("sys"."foo") [ "foo"."c2" ],
| | | table("sys"."bar") [ "bar"."c2" ]
| | ) [ ("foo"."c2") = ("bar"."c2") ]
| ) [ "foo"."c2" NOT NULL ] [ "foo"."c2" NOT NULL as "col", "sys"."count"() NOT NULL as "%1"."%1" ]
) [ "col" NOT NULL, "%1"."%1" NOT NULL ]

