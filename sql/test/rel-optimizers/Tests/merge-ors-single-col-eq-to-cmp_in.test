where n = 1
   or n = 2
   or m = 60
----
project (
| select (
| | table("sys"."f") [ "f"."n", "f"."m" ]
| ) [ (("f"."n") in (int(4) "1", int(4) "2")) or (("f"."m") = (int(8) "60")) ]
) [ "f"."n", "f"."m" ]

