## chained ors with lhs in [n, (n && m)]

query T nosort
plan select * from f
where (n = 3 and m = 60)
   or n = 1
   or n = 2
----
project (
| select (
| | table("sys"."f") [ "f"."n", "f"."m" ]
| ) [ (("f"."n") in (int(4) "1", int(4) "2")) or (("f"."n") = (int(4) "3"), ("f"."m") = (int(8) "60")), ("f"."n") in (int(4) "3", int(4) "1", int(4) "2") ]
) [ "f"."n", "f"."m" ]

