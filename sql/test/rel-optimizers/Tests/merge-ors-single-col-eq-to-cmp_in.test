query T nosort
plan select * from f
where n = 1
   or n = 2
   or n = 3
----
project (
| select (
| | table("sys"."f") [ "f"."n", "f"."m" ]
| ) [ ("f"."n") in (int(4) "1", int(4) "2", int(4) "3") ]
) [ "f"."n", "f"."m" ]

