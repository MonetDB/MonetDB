	v2	int
)

statement ok
create table fk (
	id	int generated always as identity not null primary key,
	fk1	int references pk1(pk1),
	fk2	int references pk2(pk2)
)

statement ok
create view v0 as (
	select * from fk
)

statement ok
create view v1 as (
	select * from fk
	left outer join pk1 on fk.fk1 = pk1.pk1
)

statement ok
create view v2 as (
	select * from fk
	left outer join pk1 on fk.fk1 = pk1.pk1
	left outer join pk2 on fk.fk2 = pk2.pk2
)

