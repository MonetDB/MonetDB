create table mtcars (
    mpg DOUBLE PRECISION,
    cyl DOUBLE PRECISION,
    disp DOUBLE PRECISION,
    hp DOUBLE PRECISION,
    drat DOUBLE PRECISION,
    wt DOUBLE PRECISION,
    qsec DOUBLE PRECISION,
    vs DOUBLE PRECISION,
    am DOUBLE PRECISION,
    gear DOUBLE PRECISION,
    carb DOUBLE PRECISION
);

copy into mtcars from '$QTSTSRCDIR/mtcars' using delimiters 'a','b','c' null as '' fwf (4, 2, 6, 4, 5, 6, 6, 2, 2, 2, 2);

select * from mtcars;

drop table mtcars;
