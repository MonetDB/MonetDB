CREATE MERGE TABLE ratings (movie_id BIGINT, release_year INT, title VARCHAR(200)) PARTITION BY RANGE ON (release_year);
CREATE TABLE subt1 (movie_id BIGINT, release_year INT, title VARCHAR(200));
