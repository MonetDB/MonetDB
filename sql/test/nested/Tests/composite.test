statement ok
create type myt as (n int, m text)

statement ok
create table ct_col (c myt)

## array syntax

statement ok
insert into ct_col values ((10, 'test'))

statement ok
insert into ct_col values ((20, 'alice')), ((30, 'bob'))

query I nosort
select count(*) from ct_col
----
3

query IT nosort
select c.n, c.m from ct_col
----
10
test
20
alice
30
bob

## literal syntax

statement ok
insert into ct_col values ('{(0, "head")}')

statement ok
insert into ct_col values ('{(1, "hello")}'), ('{(2, "world")}')

query IT nosort
select c.n, c.m from ct_col
----
10
test
20
alice
30
bob
0
head
1
hello
2
world

## modify table

statement ok
truncate ct_col

query I nosort
select count(*) from ct_col
----
0
