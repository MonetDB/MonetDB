statement ok
CREATE FUNCTION f2(x int)
RETURNS int
BEGIN
	IF f1(x) > 0
		THEN RETURN 1;
	END IF;
	IF f1() < 0
		THEN RETURN 2;
	ELSE
		RETURN 3;
	END IF;
END

statement ok
CREATE FUNCTION f3()
RETURNS int
BEGIN
	IF f1() < 0
		THEN RETURN 1;
	ELSE
		RETURN 2;
	END IF;
END

