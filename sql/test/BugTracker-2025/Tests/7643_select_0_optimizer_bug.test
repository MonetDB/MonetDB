-- query without the (select 0) query parts
query TTT nosort
SELECT acol1,
acol2,
SPLIT_PART(bcol4, '|', 4) as rate
FROM (
    SELECT a.col1 as acol1,
	a.col2 as acol2,
	a.col3 as acol3,
	b.col1 as bcol1,
	b.col4 as bcol4
    FROM table_a a
    INNER JOIN table_b b
	ON a.col1 = b.col1
	where a.col2 = 'INST1'
) abc
order by rate asc
LIMIT 2 offset 1
