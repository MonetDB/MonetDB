query I rowsort
with "cp_t" as
(
  select
    "cn_t"."A",
    "cn_t"."B",
    "cn_t"."C",
    "cn_t"."D"
  from
    "unitTestDontDelete" as "cn_t"
  where
    (
      "B" is null
      or "B"   <> 8
    )
)
select
  (
    select
      coalesce(sum("B"), 0)
    from
      "cp_t" as "cr_t"
    where
      (
        "cr_t"."C" >=
        (
          select
            coalesce(MEDIAN("B"),0)
          from
            "cp_t" as "cq_t"
          where
            (
              "cq_t"."A" = "cp_t"."A"
              or
              (
                "cq_t"."A"     is null
                and "cp_t"."A" is null
              )
            )
        )
      )
  )
  as "co_f1"
from
    "cp_t"
order by "co_f1"
