RETURNS TABLE(id int)
BEGIN
	DECLARE TABLE f1_t1(id int);
	DECLARE x int;
	SET x = 3;
	IF f1(x) > 0
		THEN RETURN f1_t1;
	END IF;
	INSERT INTO f1_t1 VALUES(1);
	IF f1(x) < 0
		THEN RETURN f1_t1;
	END IF;
