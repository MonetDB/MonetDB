project (
| semijoin (
| |  [  [ int(32) "1", int(32) "2", int(32) "3" ] as "y"."y" ],
| | table("sys"."testkeys") [ "testkeys"."a" NOT NULL UNIQUE HASHCOL  ]
| ) [ ("y"."y") = ("testkeys"."a" NOT NULL HASHCOL ) ]
) [ "y"."y" ]

