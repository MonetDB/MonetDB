			if (rel->name)
				rel_set_name(r, rel->name); 
			rel_destroy(rel);
			//rel_print(sql, r, 0);
			return r;

		case op_except: 
		case op_select: 
			/* simple (merge exp lists) (but shouldn't happen) */
			assert(0);
			
		case op_project:

			/* here we need to fix aliases */

//			r = r->l;
			/* for each exp check if we can rename it */
/*
