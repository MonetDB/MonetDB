interval_type:
    INTERVAL interval_qualifier	{
		int sk, ek;
		mvc *m = (mvc*)parm;
	  	int tpe;

		$$ = NULL;
	  	if ( (tpe = parse_interval_qualifier( m, $2, &sk, &ek )) < 0){
			yyerror("incorrect interval");
			YYABORT;
	  	} else {
			if (tpe == 0){
				$$ = sql_bind_subtype("month_interval", ek, sk);
			} else {
				$$ = sql_bind_subtype("sec_interval", ek, sk);
			}
	  	}
	}
 ;

user:
