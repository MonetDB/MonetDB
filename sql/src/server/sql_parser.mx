			{ if ($3)
				$$ = sql_bind_subtype("timetz", $2, 0); 
			  else
				$$ = sql_bind_subtype("time", $2, 0); 
			}
 |  TIMESTAMP timestamp_precision tz 
			{ if ($3)
				$$ = sql_bind_subtype("timestamptz", $2, 0); 
			  else
				$$ = sql_bind_subtype("timestamp", $2, 0); 
			}
