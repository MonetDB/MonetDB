literal:
    string
		{ char *s = sql2str($1);
		  int len = strlen(s);
		  sql_subtype *t = sql_bind_subtype("char", len, 0 );
		  $$ = _newAtomNode( _atom_string(t, s)); }
 |  user
		{ mvc *m = (mvc*)parm;
		  sql_subtype *t = sql_bind_subtype("char", strlen(m->user),0);
		  $$ = _newAtomNode( 
			_atom_string(t,sql2str(sa_strdup(SA, m->user))));
		}
 |  sqlINT
		{ int digits = strlen($1), err = 0, len = sizeof(lng);
		  lng value, *p = &value;
		  sql_subtype *t = NULL;

		  lngFromStr($1, &len, &p);
		  if (value == lng_nil)
		  	err = 2;

		  /* find the most suitable data type for the given number */
		  if (!err) {
		    int bits = digits2bits(digits);
		    if (value >= GDK_sht_min && value <= GDK_sht_max)
		  	  t = sql_bind_subtype("smallint", bits, 0 );
		    else if (value >= GDK_int_min && value <= GDK_int_max)
		  	  t = sql_bind_subtype("int", bits, 0 );
		    else if ((value >= GDK_lng_min && value <= GDK_lng_max))
		  	  t = sql_bind_subtype("bigint", bits, 0 );
		    else
			  err = 1;
		  }

		  if (err) {
			char buf[BUFSIZ];

			snprintf(buf, BUFSIZ, "integer value too large or not a number (%s)", $1);
			yyerror(buf);
			$$ = NULL;
			YYABORT;
		  } else {
		  	$$ = _newAtomNode( atom_int(t, value));
		  }
		}
 |  INTNUM
		{ char *s = strip_extra_zeros($1);
		  char *dot = strchr(s, '.');
		  int digits = strlen(s) - 1;
		  int scale = digits - (dot-s);
		  lng value = decimal_from_str(s);
		  sql_subtype *t = NULL;

		  if (*s == '+' || *s == '-')
			digits --;
		  t = sql_bind_subtype("decimal", digits, scale );
		  $$ = _newAtomNode( atom_int(t, value)); }
 |  APPROXNUM
		{ sql_subtype *t = sql_bind_subtype("double", 51, 0 );
		  double val = strtod($1,NULL);
		  $$ = _newAtomNode( atom_float(t, val)); }
 |  sqlDATE string
		{ sql_subtype *t = sql_bind_subtype("date", 0, 0 );
		  atom *a = atom_general(t, $2, 0);
		  if (!a) {
			char buf[BUFSIZ];

			snprintf(buf, BUFSIZ, "incorrect date value (%s)", $2);
			yyerror(buf);
			$$ = NULL;
			YYABORT;
		  } else {
		  	$$ = _newAtomNode(a);
		} }
 |  TIME tz string
		{ sql_subtype *t = sql_bind_subtype(($2)?"timetz":"time", 0, 0);
		  atom *a = atom_general(t, $3, 0);
		  if (!a) {
			char buf[BUFSIZ];

			snprintf(buf, BUFSIZ, "incorrect time value (%s)", $3);
			yyerror(buf);
			$$ = NULL;
			YYABORT;
		  } else {
		  	$$ = _newAtomNode(a);
		} }
 |  TIMESTAMP tz string
		{ sql_subtype *t = 
			sql_bind_subtype(($2)?"timestamptz":"timestamp", 0, 0 );
		  atom *a = atom_general(t, $3, 0);
		  if (!a) {
			char buf[BUFSIZ];

			snprintf(buf, BUFSIZ, "incorrect timestamp value (%s)", $3);
			yyerror(buf);
			$$ = NULL;
			YYABORT;
		  } else {
		  	$$ = _newAtomNode(a);
		} }
 |  interval_expression
 |  aTYPE string
		{ sql_subtype *t = sql_bind_subtype($1, 0, 0);
		  atom *a = atom_general(t, $2, 0);
		  $$ = NULL;
	          if (a)
			$$ = _newAtomNode(a);
		  if (!$$) {
			char buf[BUFSIZ];

			snprintf(buf, BUFSIZ, "incorrect %s %s", $1, $2);
			yyerror(buf);
			if (a)
				atom_destroy(a);
			YYABORT;
		  }
		}
 | type_alias string
		{ sql_subtype *t = sql_bind_subtype($1, 0, 0);
		  atom *a = atom_general(t, $2, 0);
		  $$ = NULL;
	          if (a)
			$$ = _newAtomNode(a);
		  if (!$$) {
			char buf[BUFSIZ];

			snprintf(buf, BUFSIZ, "incorrect %s %s", $1, $2);
			yyerror(buf);
			if (a)
				atom_destroy(a);
			YYABORT;
		  }
		}
 | IDENT string
		{ mvc *m = (mvc*)parm;
		  sql_type *t = mvc_bind_type(m, $1);
		  atom *a;

		  $$ = NULL;
		  if (t) {
			a = atom_general( sql_create_subtype(t, 0, 0), $2, 0);
			if (a)
				$$ = _newAtomNode(a);
		  }
		  if (!t || !$$) {
			char buf[BUFSIZ];

			snprintf(buf, BUFSIZ, "type (%s) unknown", $1);
			yyerror(buf);
			YYABORT;
		  }
		}
 ;

interval_expression:
   INTERVAL opt_sign string interval_qualifier { 
		sql_subtype *t;
		int sk, ek, tpe;
		mvc *m = (mvc*)parm;
	  	lng i = 0;

		$$ = NULL;
	  	if ( (tpe = parse_interval_qualifier( m, $4, &sk, &ek )) < 0){
			yyerror("incorrect interval");
			YYABORT;
	  	} else {
			if (tpe == 0){
				t = sql_bind_subtype("month_interval", ek, sk);
			} else {
				t = sql_bind_subtype("sec_interval", ek, sk);
			}
	  	}
	  	if ( (tpe = parse_interval( m, $2, $3, sk, ek, &i)) < 0 ){
			yyerror("incorrect interval");
			$$ = NULL;
			YYABORT;
	  	} else {
	  		$$ = _newAtomNode( atom_int(t, i));
	  	}
	}

	/* miscellaneous */
 ;

qname:
