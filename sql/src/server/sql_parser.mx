	'(' opt_paramlist ')'
    AS			{ mvc *m = (mvc*)parm;
			  m->scanner.as = m->scanner.yycur; }
    select_stmt		/* todo stmt set BEGIN/END */
			{ dlist *f = _dlist_create();
