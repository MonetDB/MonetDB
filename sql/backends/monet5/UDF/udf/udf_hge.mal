# scalar MAL signatures
module udf;

command fuse(one:lng,two:lng):hge
address UDFfuse_lng_hge
comment "fuse two (8-byte) lng values into one (16-byte) hge value";


# BAT MAL signatures
module batudf;

command fuse(one:bat[:lng],two:bat[:lng]):bat[:hge]
address UDFBATfuse
comment "fuse two (8-byte) lng values into one (16-byte) hge value";
