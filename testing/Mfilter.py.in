##  r'^(bad usage of XML namespaces: at )(?:.*)\n',                                                                                             # 19: 1
    r"^(QUERY|ERROR)( =.* connect)( to|)( ')(localhost)(' port )([0-9]+)( .*)\n",                                                               # 21: 8
    r"^([Uu]sage: |)(/.*/\.libs/|/.*/lt-|)([A-Za-z0-9_]+:?[ \t].*)\n",                                                                          # 22: 3
