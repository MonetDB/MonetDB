
# remapping inline functions
function foo{inline}(a:str):str;
	a:= str.replace(a,"A","");
	a:= str.replace(a,"E","");
	a:= str.replace(a,"I","");
	a:= str.replace(a,"O","");
	a:= str.replace(a,"U","");
	a:= str.replace(a," ","");
	return foo:=a;
end foo;

function bar{inline}(a:str,b:str):str;
	sa:= user.foo(a);
	sa:= str.substring(sa, 1, 5);
	sb:= user.foo(b);
	sb:= str.substring(sb, 1, 3);
	c:= sa+sb;
	return bar:=c;
end bar;

