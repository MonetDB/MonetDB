# GDK layer should produce exceptions rather then direct warnings
# or ignoring.
# a typical case is to try to rename an existing (persistent) bat

	b:bat[:int,:int]:= bat.new(:int,:int);
	bat.setName(b,"mybat");
	#setPersistent(b);
	bat.setColumn(b,"first","second");
	io.print(b);
	c:= bat.new(:int,:str);
	bat.setName(c,"mybat");
	io.print(c);
	#setPersistent(c);
	io.print(c);
	io.printf("catched anything?\n");
catch GDKerror;
	io.printf("Catched the renaming exception\n");
exit  GDKerror;
#get rid of it again
	#destroy(b);
	#destroy(c);
