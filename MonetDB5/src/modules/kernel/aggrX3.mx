    }
	if( CMDaggrX3_sum_@1_@3(&bn,b,e) == GDK_SUCCEED){
		BBPunfix(b->batCacheid);
		BBPunfix(e->batCacheid);
		BBPincref(*retid = bn->batCacheid,TRUE);
		BBPunfix(*retid);
		return MAL_SUCCEED;
	}
