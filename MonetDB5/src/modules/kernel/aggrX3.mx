str AX3aggrX3_sum_@1_@3(int *retid, int *bid, int *eid){
	BAT *b, *e;
	BAT *bn;

    if( (b= BATdescriptor(*bid)) == NULL ){
        return throwMessage("bbp.sum", "Cannot access descriptor");
    }
    if( (e= BATdescriptor(*eid)) == NULL ){
		BBPunfix(b->batCacheid);
