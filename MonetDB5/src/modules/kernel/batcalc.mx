#define NULLTST
@-
The accumulator operators are relatively straight forward,
when the target BAT is known and we only have to deal with
fixed-sized types. 
@= batinit
	s@1= BUNsize(@2);
	p@1= (str) (BUNfirst(@2)+@2->tloc);
@-
@= accum
	while( p1 < hend ){
		/* this code is better as x?nil:y  */
#ifdef NULLTST
		if(*(@3 *)p2 == @3_nil || *(@4*)p3 == @4_nil)
			*(@1*)p1= @1_nil;
		else 
#endif
			*(@1*)p1= (@1) (* (@3*) p2 @2 *(@4 *) p3);
		p1= (ptr)( ((char*)p1) + s1);
		p2= (ptr)( ((char*)p2) + s2);
		p3= (ptr)( ((char*)p3) + s3);
	}
@= accumDIV
	while( p1 < hend ){
		/* this code is better as x?nil:y  */
#ifdef NULLTST
		if(*(@3 *)p2 == @3_nil || *(@4*)p3 == @4_nil || *(@4*)p3== 0)
			*(@1*)p1= @1_nil;
		else 
#endif
			*(@1*)p1= (@1)(* (@3*) p2 @2 *(@4 *) p3);
		p1= (ptr)( ((char*)p1) + s1);
		p2= (ptr)( ((char*)p2) + s2);
		p3= (ptr)( ((char*)p3) + s3);
	}
@-
This code could be further improved by factoring
out occurrences of the nil-tst on the constant provided.
This has been left out.
@= accumCst
	while( p1 < hend ){
#ifdef NULLTST
		if(*(@1 *)p2 == @1_nil || *(@1*)cst == @1_nil)
			*(@1*)p1= @1_nil;
		else 
#endif
			*(@1*)p1 = *(@1*) p2 @2 *(@1 *) cst;
		p1= (ptr)( ((char *)p1) + s1);
		p2= (ptr)( ((char *)p2) + s2);
	}
@= accumCstLeft
	while( p1 < hend ){
#ifdef NULLTST
		if(*(@1 *)p2 == @1_nil || *(@1*)cst == @1_nil)
			*(@1*)p1= @1_nil;
		else 
#endif
			*(@1*)p1 = *(@1*) cst @2 *(@1 *) p2;
		p1= (ptr)( ((char *)p1) + s1);
