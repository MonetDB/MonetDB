@= accumUnary
	{
		@1* p = (@1*)Tloc(b,BUNfirst(b));
		@1 *q = (@1*)Tloc(b,BUNlast(b));
		@1 *o = (@1*)Tloc(bn,BUNfirst(bn));

		if (b->T->nonil)
			for(; p<q; p++, o++)
				*o = @2(*p);
		else
			for(; p<q; p++, o++){
				/* this code is better as x?nil:y  */
				if(*p == @1_nil ) {
					*o = @1_nil;
					bn->T->nonil = 0;
				} else 
					*o = @2(*p);
			}
	}
