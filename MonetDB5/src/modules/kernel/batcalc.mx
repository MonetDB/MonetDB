	if (b->T->nonil && right->T->nonil) {
		for (;p<q; p++, t++, o++)
			*o = (*p @2 *t);
	} else if (b->T->nonil) {
		for (;p<q; p++, t++, o++) {
			if (*t == @3_nil) {
#if OPERATOR_@1 == OPERATOR_OR
