		ptr t = BUNtail(b,p);

		if( ATOMcmp(tpe, t, nilptr)==0 ){
			bunfastins(bn, h, t);
		} else {
			bit match=  regexec(&prg,t,0,0,0)==0;
			bunfastins(bn, h, &match);
		}
	}
bunins_failed:
	BBPfix( *ret= bn->batCacheid);
        return MAL_SUCCEED;
}

