		while(p<q){
			if( *(bit*)p == bit_nil)
				*o++ = bit_nil;
			else 
				*o++ = ! *(bit*)p;
			p += xx;  
		}
		(bn)->batBuns->free += BATcount(b)*sizeof(bit);
		BATsetcount(bn, BATcount(b));
	} else {
		BUN p,q;
		size_t xx;
		BATloopFast(b, p, q, xx) {
