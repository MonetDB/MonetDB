@-
@= Enlarge
str CMDconvert@1_@2Enlarge(int *ret, int *bid){
	BAT *b,*bn;
	int xx;
	BUN p,q;
	@1 x;
	ptr p1; int s1;

	@:getBATdescriptor(ret,bid,b,CMDconvert@1)@

	if( b->htype!= TYPE_void) 
		return CMDconvert@1_@2(ret,bid);
	bn= newBATresult(b, TYPE_@2);
	@:tstBat(CMDconvert@1_@2)@

	@:batinit(1,bn)@
	BATloopFast(b, p, q, xx) {
		x= *(@1 *) BUNtail(b,p);
		if( x== @1_nil)
			*(@2 *)p1 = @2_nil;
		else *(@2 *) p1 = (@2) x;
		p1 = (ptr) (((char *) p1)+s1);
		(bn)->batBuns->free += s1;
	}
	BBPfix( *ret= bn->batCacheid);
	return MAL_SUCCEED;
}
@= General
str CMDconvert@1_@2(int *ret, int *bid){
	BAT *b,*bn;
	int xx;
	BUN p,q;
	@1 x;
	@2 y;

	@:getBATdescriptor(ret,bid,b,CMDconvert@1)@

	bn= newBATresult(b, TYPE_@2);
	@:tstBat(CMDconvert@1)@

	BATloopFast(b, p, q, xx) {
		ptr h = BUNhead(b,p);
		x= *(@1 *) BUNtail(b,p);
		y= x== @1_nil? @2_nil: x;
		bunfastins(bn, h, &y);
	}
bunins_failed:
	BBPfix( *ret= bn->batCacheid);
	return MAL_SUCCEED;
}
