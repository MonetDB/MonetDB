str
CMDbitbatNOT(int *ret, int *bid)
{
	BAT *bn, *b;
	bit nilval = bit_nil;

	@:getBATdescriptor(bid,b,"batcalc.not")@
	@:resultBAT(bit,"batcalc.not")@

	if (b->htype == TYPE_void ){
		bit *o = (bit*) BUNtail(bn,BUNfirst(bn));
